{
  "name": "webpack_boilerplate",
  "version": "1.0.0",
  "description": "A simple but effective webpack boilerplate",
  "main": "index.js",
  "scripts": {
    "start": "webpack serve --config config/webpack.dev.js",
    "build:dev": "webpack --config config/webpack.dev.js",
    "build:prod": "webpack --config config/webpack.prod.js",
    "prebuild:dev": "clear & rm -rf dist",
    "prebuild:prod": "clear & rm -rf dist",
    "lint:js": "clear & eslint src/apps/ *.js --cache",
    "lint:jsfix": "clear & eslint src/apps/ *.js --cache --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/and-ocean2017/webpack-boilerplate.git"
  },
  "keywords": [],
  "author": "Praveen kumar",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/and-ocean2017/webpack-boilerplate/issues"
  },
  "homepage": "https://github.com/and-ocean2017/webpack-boilerplate#readme",
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^5.0.1",
    "eslint": "^7.17.0",
    "html-webpack-plugin": "^5.0.0-beta.4",
    "mini-css-extract-plugin": "^1.3.3",
    "node-sass": "^5.0.0",
    "sass-loader": "^10.1.0",
    "style-loader": "^2.0.0",
    "webpack": "^5.11.1",
    "webpack-cli": "^4.3.1",
    "webpack-dev-server": "^3.11.1",
    "webpack-merge": "^5.7.3"
  },
  "dependencies": {
    "bootstrap": "^4.5.3",
    "jquery": "^3.5.1",
    "popper.js": "^1.16.1"
  }
}
